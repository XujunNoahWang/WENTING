{
  "name": "wenting-backend",
  "version": "1.0.0",
  "description": "雯婷后端API服务",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node scripts/init-todo-db.js",
    "clear-db": "node scripts/clear-database.js",
    "reset-db": "npm run clear-db && npm run init-db",
    "test": "jest",
    "test:unit": "jest tests/linkService.test.js",
    "test:integration": "node scripts/run-integration-tests.js",
    "test:e2e": "jest tests/e2e.test.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
